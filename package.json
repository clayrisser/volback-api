{
  "name": "volback-api",
  "version": "0.0.1",
  "description": "rest api for volback docker volume backup software",
  "keywords": [
    "api",
    "backup",
    "bivac",
    "database",
    "docker",
    "dump",
    "loopback",
    "loopback-application",
    "persist",
    "rest",
    "volback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build": "lb-tsc es2017 --outDir dist",
    "build:apidocs": "lb-apidocs",
    "build:watch": "lb-tsc --watch",
    "clean": "git clean -fXd -e \\!node_modules -e \\!node_modules/**/*",
    "docker:build": "docker build -t volback-api .",
    "docker:run": "docker run -p 3000:3000 -d volback-api",
    "format": "npm run prettier:fix",
    "lint": "npm run prettier:check && npm run tslint",
    "lint:fix": "npm run tslint:fix && npm run prettier:fix",
    "migrate": "node ./dist/migrate",
    "posttest": "npm run lint",
    "prepublishOnly": "npm run test",
    "prestart": "npm run build",
    "pretest": "npm run clean && npm run build",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:fix": "npm run prettier:cli -- --write",
    "start": "node .",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "tslint": "lb-tslint",
    "tslint:fix": "npm run tslint -- --fix"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^1.1.0",
    "@loopback/context": "^1.7.0",
    "@loopback/core": "^1.1.8",
    "@loopback/openapi-v3": "^1.3.0",
    "@loopback/repository": "^1.1.8",
    "@loopback/rest": "^1.8.0",
    "@loopback/rest-explorer": "^1.1.11",
    "@loopback/service-proxy": "^1.0.10",
    "loopback-connector-cli": "0.1.0",
    "loopback-connector-rest": "^3.4.1"
  },
  "devDependencies": {
    "@loopback/build": "^1.3.2",
    "@loopback/testlab": "^1.1.0",
    "@loopback/tslint-config": "^2.0.2",
    "@types/node": "^10.11.2",
    "prettier": "^1.16.4",
    "tslint": "^5.12.0",
    "typescript": "^3.3.1"
  }
}
